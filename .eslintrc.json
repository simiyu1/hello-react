{
	"env": {
	  "browser": true,
	  "es6": true,
	  "node": true,
	  "jest": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
	  "ecmaFeatures": {
		"experimentalObjectRestSpread": true,
		"jsx": true
	  },
	  "sourceType": "module"
	},
	"plugins": ["react"],
	"parser": "babel-eslint",
	"rules": {
	  "indent": [2, 2, { "SwitchCase": 1 }],
	  "linebreak-style": ["error", "unix"],
	  "quotes": ["error", "double"],
		"react/prop-types": 0,
		"no-mixed-spaces-and-tabs": [2, "smart-tabs"]
	}
  }